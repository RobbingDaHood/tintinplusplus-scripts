2024-02-16-20:43:29[22;37m             {
2024-02-16-20:43:29[22;37m                 #foreach {*hashtable[$hash1][%*]} {hash2}
2024-02-16-20:43:29[22;37m                 {
2024-02-16-20:43:29[22;37m                     #echo {%-20s = %s}
2024-02-16-20:43:29[22;37m                                        {hashtable[$hash1][$hash2]}
2024-02-16-20:43:29[22;37m                                        {$hashtable[$hash1][$hash2]}
2024-02-16-20:43:29[22;37m                 }
2024-02-16-20:43:29[22;37m             }
2024-02-16-20:43:29        }
